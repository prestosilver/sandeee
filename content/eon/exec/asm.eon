#include "content/eon/libs/libload.eon"
#include "content/eon/libs/asm.eon"

fn processArgs() {
    var file = getArg(1);

    if (file == 0) {
        error("Bad Argument");
    }

    return file;
}

fn main() {
    // load libs
    setupLibLoad();
    loadLib("/libs/string.ell");

    // open input
    var infile = processArgs();

    asmFile(infile, "out.eep");

    return 0;
}
