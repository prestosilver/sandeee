#include "/libs/inc/libload.eon"

fn processArgs() {
    var file = 1;
    asm "sys 8";

    if (file == 0) {
        asm "push \"Bad Argument\"";
        asm "sys 18";
    }

    return file;
}

fn main() {
    // load libs
    setupLibLoad();
    loadLib("/libs/asm.ell");

    // open input
    var infile = processArgs();

    _asmFile(infile, "out.eep");

    return 0;
}
