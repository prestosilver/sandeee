fn print() {
    asm "sys 0";
    asm "push 0";
    asm "ret";
}

fn fib(idx) {
    if (idx < 2) {
        return idx;
    }

    return fib(idx - 1) + fib(idx - 2);
}

fn printfib(idx) {
    var val = fib(idx);

    print(idx);
    print(": ");
    print(val);
    print("\n");

    return 0;
}

fn main() {
    var i = 0;
    for (i = 0; i < 20; i = i + 1) {
        printfib(i);
    }

    return 0;
}
