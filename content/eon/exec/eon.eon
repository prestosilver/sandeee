#include "/libs/inc/libload.eon"
#include "/libs/inc/sys.eon"

fn main() {
    setupLibLoad();
    loadLib("/libs/eon.ell");

    var fileName = getArg(1);
    var outName = "out.eep";

    if (getArg(2) != "") {
       outName = getArg(2);
    }

    _eonFile(fileName, outName, 0);
    
    return 0;
}
