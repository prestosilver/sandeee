struct TocEntry {
    u8 namelen;
    char name[namelen];
    u8 offset;
    u8 len;
};

struct Toc {
    u8 len;
    u8 count;
    TocEntry entrys[count];
};

struct ELib {
    char magic[4];
    Toc toc;
};

ELib elib @ 0x00;

u32 idx = 0;
u64 start = elib.toc.len;
struct Entry {
    char data[elib.toc.entrys[idx].len] [[name(elib.toc.entrys[idx].name)]];
    idx = idx + 1;
};

Entry entrys[elib.toc.count] @ start + elib.toc.entrys[idx].offset;
